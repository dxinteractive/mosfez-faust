{
  "name": "mosfez-faust",
  "version": "0.0.2",
  "description": "Makes dynamic compilation of Faust on the web a little easier.",
  "main": "./dist/faust.js",
  "exports": {
    "./faust": {
      "default": "./dist/faust.js",
      "types": "./dist/faust.d.ts"
    },
    "./touch-start": {
      "default": "./dist/touch-start.js",
      "types": "./dist/touch-start.d.ts"
    },
    "./convert": {
      "default": "./dist/convert.js",
      "types": "./dist/convert.d.ts"
    }
  },
  "keywords": ["faust", "wasm", "grame"],
  "homepage": "https://github.com/dxinteractive/mosfez-faust",
  "repository": {
    "type": "git",
    "url": "git@github.com:dxinteractive/mosfez-faust.git"
  },
  "bugs": {
    "url": "https://github.com/dxinteractive/mosfez-faust/issues"
  },
  "author": "Damien Clarke <dxinteractive@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prep": "yarn && cd dev && yarn",
    "build": "rm -rf dist && yarn rollup -c",
    "test": "yarn jest",
    "lint": "yarn eslint src",
    "pretty": "yarn prettier --write .",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@types/audiobuffer-to-wav": "^1.0.2",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "esbuild": "^0.14.42",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.1.0",
    "prettier": "2.6.2",
    "rollup": "^2.75.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "public"
  ],
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0"
  }
}
